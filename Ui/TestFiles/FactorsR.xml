<?xml version="1.0" encoding="UTF-8"?>
<Menue>
 <MenueEntry Name="Factors affecting species richness">
    <Function Name="Factors" HasData="TRUE"  DefaultData="data(Sharks)" PreCommand = "@_Build_AdWorld_">
     <ExecuteCommandOnce Name="require(MapsR)"/>
     <ExecuteCommandOnce Name="data(adworld)"/>
     <Argument Name="Area" Viewer="Map" Tag="adworld1"/>
     <Argument Name="exclude" Viewer="Map" Tag="adworld2"/>
     <RequiredArgument Name="data"/>
     <RequiredArgument Name="varInteresX"/>
     <RequiredArgument Name="varInteresY"/>
     <Argument Name="varInteresX" Viewer="DataColumnsViewer" AllowMultiSelect="TRUE"/>
     <Argument Name="varInteresY" Viewer="DataColumnsViewer" AllowMultiSelect="FALSE"/>
     <FunctionArgument Name="pairs" Function="pairs" Variant="\method{pairs}{default}">
       <ReadOnlyArgument Name="x"/>
       <SetArgument Name="main" Value="NULL"/>
       <SetArgument Name="cex.main" Value="2"/>
       <SetArgument Name="lower.panel" Value="panel.smooth"/>
       <SetArgument Name="upper.panel" Value="panel.reg"/>
       <SetArgument Name="diag.panel" Value="panel.hist"/>
       <SetArgument Name="cex" Value="1.2"/>
       <SetArgument Name="pch" Value="24"/>
       <SetArgument Name="bg" Value="'#B2DFEEFF'"/>
       <SetArgument Name="col.smooth" Value="'red'"/>
       <SetArgument Name="line.col" Value="'red'"/>
       <SetArgument Name="cex.labels" Value="1"/>
       <SetArgument Name="font.labels" Value="2"/>
     </FunctionArgument>
     <FunctionArgument Name="ksvm" Function="ksvm" Variant="\S4method{ksvm}{matrix}">
       <ReadOnlyArgument Name="x"/>
       <ReadOnlyArgument Name="data"/>
       <SetArgument Name="prob.model" Value="FALSE"/>
     </FunctionArgument>
   </Function>
  </MenueEntry>
  <Separator/>
  <PDF Name="FactorsR-manual.pdf"/>
</Menue>
